drawPanel()
{    	
	float k1,k2,x,y1,y2;
	float scale; 
	string HvchClicked;
	string cellName,chNo,toolTip;
	int channelNo,CellHeight;
	int refRec1_x,refRec1_y,refRec2_x,refRec2_y;	
	getValue(\"RECTANGLE1\",\"position\",refRec1_x,refRec1_y); // get ref point of RECTANGLE1
	getValue(\"RECTANGLE2\",\"position\",refRec2_x,refRec2_y); // get ref point of RECTANGLE2
//	DebugN(\"position=\",refRec1_x,refRec1_y);	
	
	scale=1;
	CellHeight=24;
	y1=0; y2=0;
	x=0;
//--------------------------------draw_table -------------------------
	for (int i=1; i<=14; i++)
	{ 
		cellName=\"boxRow\"+i;	
		channelNo= i-1;
		if(channelNo<=9)
			chNo=\"0\"+channelNo;
		else
			chNo=channelNo;
		HvchClicked = HvBrdClicked+\"/channel0\"+chNo;
		HvchannelsDps[i]=HvchClicked;
//		DebugN(\"HvchClicked=\",HvchClicked);
//		CAEN/GEM_CAEN_HV_01/board00/channel000
//		***************show HvchClicked as tooltip**********
		toolTip=\"TEXT\"+i;
		setValue(toolTip, \"toolTipText\", HvchClicked);
//		***************---------------------------**********
		if(i<=7)
		{
			k1=addSymbol(myModuleName(),\"\", // chamber itself
				\"/CMS_GEM_CAEN/objects/chamber_settings_obj/refHv_ChangeSettingsHvBox.pnl\", 
				cellName,makeDynString(\"$dpHvchClicked:\"+HvchClicked,\"$HvBrdName:\"+HvBrdName,\"$HvBrdNo:\"+HvBrdNo,\"$channelNo:\"+channelNo),
				refRec1_x+x,refRec1_y+y1,0,scale,scale);
			
			y1=y1+CellHeight-1;
		}
		else
		{
			k2=addSymbol(myModuleName(),\"\", // chamber itself
				\"/CMS_GEM_CAEN/objects/chamber_settings_obj/refHv_ChangeSettingsHvBox.pnl\", 
				cellName,makeDynString(\"$dpHvchClicked:\"+HvchClicked,\"$HvBrdName:\"+HvBrdName,\"$HvBrdNo:\"+HvBrdNo,\"$channelNo:\"+channelNo),
				refRec2_x+x,refRec2_y+y2,0,scale,scale);
			
			y2=y2+CellHeight-1;
		}
	}
	BoardDpText.text(HvBrdClicked);
}
updateSum()
{
	float sum1=0;
	float sum2=0;
	dyn_float values1;
	dyn_float values2;
	for(int i=1; i<=7; i++)
	{
		getValue(\"V0_\"+i, \"text\", values1[i]);
		getValue(\"V0_\"+7+i, \"text\", values2[i]);
		sum1=sum1+values1[i];	
		sum2=sum2+values2[i];
	}
	setValue(\"TEXT_Sum1\", \"text\", sum1);
	setValue(\"TEXT_Sum2\", \"text\", sum2);
}
updateDpSum()
{
	dyn_errClass err;
	dyn_string Dps;
	for(int i=1; i<=14; i++)
	{
		Dps[i] = HvchannelsDps[i]+\".readBackSettings.v0:_online.._value\";
		if( !dpExists( Dps[i] ))
		{
			setValue(\"TEXT_Sum1\", \"color\", \"_dpdoesnotexist\");
			setValue(\"TEXT_Sum2\", \"color\", \"_dpdoesnotexist\");
			return;
		}
	}
	dpConnect(\"calculateSum\", Dps);
	err = getLastError();
	if (dynlen(err) > 0)
	{
		setValue(\"TEXT_Sum1\", \"color\", \"_dpdoesnotexist\"); 
		setValue(\"TEXT_Sum2\", \"color\", \"_dpdoesnotexist\"); 
	}
}
calculateSum(dyn_string dp, dyn_float Values)
{
	float sum1=0;
	float sum2=0;
	for(int i=1; i<=7; i++)
	{
		sum1=sum1+Values[i];
		sum2=sum2+Values[7+i];
	}
	setValue(\"TEXT_Sum1\", \"text\", sum1);
	setValue(\"TEXT_Sum2\", \"text\", sum2);
}
bool Crosscheck_table_dpesettings()
{
	dyn_float settings_texts;
	dyn_bool settings_cascades;
	bool changecolor=0;
	float tmp=-1;
	bool tmpbool=-1;
	string tmpstring=\"\";
	settings_cascades = makeDynBool( 0 , 0 , 0 , 0 , 0 , 0 , 0);
	
	for (int i=1; i<=98; i++) 
	{
		settings_texts[i]=0;
	}
	getTextBoxNameList();
	getDpList();
	
	dpGet(sDp_textBoxes , settings_texts);
	dpGet(sDpCascades , settings_cascades);
//	DebugTN(\"settings_texts=\", settings_texts);
//	DebugTN(\"settings_cascades=\", settings_cascades);

	for (int i=1; i<=dynlen(textBoxNames); i++)
	{
		getValue(textBoxNames[i],\"text\",tmp);
		if (settings_texts[i] != tmp) changecolor=1;
		tmp=-1;
//		DebugTN(\"changecolor=\",changecolor,\"   i=\",i);
	}
	for (int j=1; j<=dynlen(cascadeNames); j++)
	{
		getValue(cascadeNames[j],\"text\",tmpstring);
		if ( tmpstring == \"On\" ) tmpbool=1;
		if ( tmpstring == \"Off\" ) tmpbool=0;
		if (settings_cascades[j] != tmpbool) changecolor=1;
		tmp=-1;
		tmpbool=-1;
		tmpstring=\"\";
//		DebugTN(\"changecolor=\",changecolor,\"   j=\",j);
	}
	return changecolor; //this value valid for changecolor and showmessage
}
