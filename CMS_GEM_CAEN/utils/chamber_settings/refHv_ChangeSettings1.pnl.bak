		if (answ)
			applyChanges();
	}
	else
	{
		if(hasModify)
		{
			bool answ3;    
			fwGeneral_openMessagePanel(\"One or more LV channels connected to this supper chambers is/are OFF. Do you really want to apply changes to the HV?\", answ3 , exceptionInfo, \"Confirm\", false );
