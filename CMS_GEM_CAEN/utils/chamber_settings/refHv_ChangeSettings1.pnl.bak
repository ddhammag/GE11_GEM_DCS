				applyChanges();
		}
		else
		{
			bool answ2;
			fwGeneral_openMessagePanel(\"One or more LV channels connected to this supper chambers is/are OFF, you cannot change HV status! Call an expert!\", answ2 , exceptionInfo, \"Error!\", true );    
