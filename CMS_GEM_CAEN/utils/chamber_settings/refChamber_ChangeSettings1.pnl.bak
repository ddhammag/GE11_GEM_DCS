	if (text_OpMode == \"FREE\")  tmpbool = FALSE ;
	if (text_OpMode == \"GEM\")  tmpbool = TRUE  ;
  
	if (setting_OpMode != tmpbool) showmessage = 1;
//	DebugTN(\"setting_OpMode\",setting_OpMode, \"    tmpobool\",tmpbool);
	
	return showmessage;
}
