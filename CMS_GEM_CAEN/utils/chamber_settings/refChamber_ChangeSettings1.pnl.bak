	LvChLayer1Dp= uniSubStr(LvchClicked, 0, ((int)strlen(LvchClicked)-1))+LvChLayer1No;
	LvChLayer2Dp= uniSubStr(LvchClicked, 0, ((int)strlen(LvchClicked)-1))+LvChLayer2No;
