	fwAccessControl_isGranted( \"CMS_GEM:Modify\", hasModify, exceptionInfo); 
	if(LVstatus1&&LVstatus2)
	{
		bool answ;
		fwGeneral_openMessagePanel(\"Do you really want to apply changes?\", answ , exceptionInfo, \"Confirm\", false );
//		DebugTN(\"answer\",answ);  
		if (answ)
			applyChanges();
	}
	else
	{
		if(hasModify)
		{
			bool answ3;    
			fwGeneral_openMessagePanel(\"Layer 1 or Layer 2 LV of this supper chamber is OFF. Do you really want to apply changes to the HV?\", answ3 , exceptionInfo, \"Confirm\", false );
//			DebugTN(\"answer\",answ3);
			if (answ3)
				applyChanges();
		}
		else
		{
			bool answ2;
			fwGeneral_openMessagePanel(\"Layer 1 or Layer 2 LV of this supper chamber is OFF, you cannot change HV status! Call an expert!\", answ2 , exceptionInfo, \"Error!\", true );    
//			DebugTN(\"answer\",answ2);
		}
	}
}

