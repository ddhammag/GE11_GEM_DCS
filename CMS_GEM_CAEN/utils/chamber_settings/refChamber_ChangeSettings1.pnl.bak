drawPanel()
{    
	
	float k1,k2,x,y;
	float scale; 
	string HvchClicked;
	string cellName,layerName,chNo,ChmbrNo,ChmbrName,toolTip;
	int channelNo,CellHeight,GEMfoilNo;
	int j;
	int refRec1_x,refRec1_y;	
	getValue(\"RECTANGLE1\",\"position\",refRec1_x,refRec1_y); // get ref point of RECTANGLE1
//	DebugN(\"position=\",refRec1_x,refRec1_y);	
	
 
	scale=1;
	CellHeight=24;
	y=0;
	x=0;
	if(ChamberNo%2==0)
		j=7;
	else if(ChamberNo%2==1)
		j=0;
//--------------------------------draw_table -------------------------
	for (int i=1; i<=7; i++)
	{ 
		cellName=\"boxRow\"+i;	
		GEMfoilNo=i;
		channelNo=j+(7-i);
		if(channelNo<=9)
			chNo=\"0\"+channelNo;
		else
			chNo=channelNo;
		HvchClicked = HvBrdClicked+\"/channel0\"+chNo;
		HvchannelsDps[i]=HvchClicked;
//		DebugN(\"HvchClicked=\",HvchClicked);
//		CAEN/GEM_CAEN_HV_01/board00/channel000
//		***************show HvchClicked as tooltip**********
		toolTip=\"TEXT\"+i;
		setValue(toolTip, \"toolTipText\", HvchClicked);
//		***************---------------------------**********
		k1=addSymbol(myModuleName(),\"\", // chamber itself
			\"/CMS_GEM_CAEN/objects/chamber_settings_obj/refChamber_ChangeSettingsHvBox.pnl\", 
			cellName,makeDynString(\"$dpHvchClicked:\"+HvchClicked,\"$ChamberName:\"+ChamberName,\"$ChamberNo:\"+ChamberNo,\"$Layer:\"+Layer,\"$GEMfoilNo:\"+GEMfoilNo),
			refRec1_x+x,refRec1_y+y,0,scale,scale);		
			
		y=y+CellHeight-1;	
	}
	BoardDpText.text(HvBrdClicked);
}
updateSum()
{
	float sum=0;
	dyn_float values;
	for(int i=1; i<=7; i++)
	{
		getValue(\"V0_\"+i, \"text\", values[i]);
		sum=sum+values[i];	
	}
	setValue(\"TEXT_Sum\", \"text\", sum );
}
updateDpSum()
{
	dyn_errClass err;
	dyn_string Dps;
	for(int i=1; i<=7; i++)
	{
		Dps[i] = HvchannelsDps[i]+\".readBackSettings.v0:_online.._value\";
		if( !dpExists( Dps[i] ))
		{
			setValue(\"TEXT_Sum\", \"color\", \"_dpdoesnotexist\");
			return;
		}
	}
	dpConnect(\"calculateSum\", Dps);
	err = getLastError();
	if (dynlen(err) > 0)
		setValue(\"TEXT_Sum\", \"color\", \"_dpdoesnotexist\"); 
}
calculateSum(dyn_string dp, dyn_float Values)
{
	float sum=0;
	for(int i=1; i<=7; i++)
		sum=sum+Values[i];
	setValue(\"TEXT_Sum\", \"text\", sum );
}
bool Crosscheck_table_dpesettings()
{
	dyn_float settings_texts;
	dyn_bool settings_cascades;
	bool changecolor=0;
	float tmp=-1;
	bool tmpbool=-1;
	string tmpstring=\"\";
	settings_cascades = makeDynBool( 0 , 0 , 0 , 0 , 0 , 0 , 0);
	
	for (int i=1; i<=49; i++) 
	{
		settings_texts[i]=0;
	}
	getTextBoxNameList();
	getDpList();
	
	dpGet(sDp_textBoxes , settings_texts);
	dpGet(sDpCascades , settings_cascades);
//	DebugTN(\"settings_texts=\", settings_texts);
//	DebugTN(\"settings_cascades=\", settings_cascades);

	for (int i=1; i<=dynlen(textBoxNames); i++)
	{
		getValue(textBoxNames[i],\"text\",tmp);
		if (settings_texts[i] != tmp) changecolor=1;
		tmp=-1;
//		DebugTN(\"changecolor=\",changecolor,\"   i=\",i);
	}
	for (int j=1; j<=dynlen(cascadeNames); j++)
	{
		getValue(cascadeNames[j],\"text\",tmpstring);
		if ( tmpstring == \"On\" ) tmpbool=1;
		if ( tmpstring == \"Off\" ) tmpbool=0;
		if (settings_cascades[j] != tmpbool) changecolor=1;
		tmp=-1;
		tmpbool=-1;
		tmpstring=\"\";
//		DebugTN(\"changecolor=\",changecolor,\"   j=\",j);
	}
	return changecolor; //this value valid for changecolor and showmessage
}
