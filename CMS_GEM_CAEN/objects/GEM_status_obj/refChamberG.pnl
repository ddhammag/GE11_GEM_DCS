{
  string tag=Endcap+\"G_\";   
  //DebugN(\"First dp is=\"+dpl[1]);
  tag=tag+cellNo; 
  //DebugN(\"First dp is=\"+dpl[1]);   
  if(sval==0)
    setValue(tag,\"backCol\",\"FwStateOKNotPhysics\");
