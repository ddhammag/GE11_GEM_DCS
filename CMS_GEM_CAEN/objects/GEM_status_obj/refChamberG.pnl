    setValue(tag,\"backCol\",\"FwStateOKPhysics\");
  if(sval==3)  
    setValue(tag,\"backCol\",\"FwStateAttention1\");
  if(sval==5)  
    setValue(tag,\"backCol\",\"FwStateAttention1\");
  if(sval > 8)  
    setValue(tag,\"backCol\",\"FwStateAttention3\");  
}" 0
