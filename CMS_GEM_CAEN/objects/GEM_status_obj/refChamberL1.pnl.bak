ChmbrInOrEx(string dpl,bool status)
{
	string tag; 
	tag=Endcap+\"L1_\"+L1CellNo; 
	if(status)
	{
		ChmbrInOrExStatus=TRUE;
		setValue(tag,\"backCol\",\"_Transparent\");	
	}
	else
	{
		ChmbrInOrExStatus=FALSE;
		setValue(tag,\"backCol\",\"FwEquipmentDisabled\"); 
	}
